  <!-- Header -->
  <header class="masthead">
    <% if user_signed_in? %>
      <br>
      <% if current_user.has_any_role?(:admin, :secretaria) %>
        <%= render "secretary_dashboard" %>
      <% end %>
      <% if current_user.has_any_role?(:docente)%>
        <%= render "teacher_dashboard"%>
      <% end %>
    <% else %>
        <div class="d-flex flex-column p-2 mx-auto">
          <h1 class="d-flex p-2 mx-auto my-0 text-uppercase">AUSMA</h1>
          <div class="d-flex p-2">
            <h2 class="text-white-50 mx-auto mt-2 mb-5">Universidad Nacional del Comahue</h2>
          </div>
        </div>
      <%= render "devise/sessions/new_session" %>
    <% end %>
  </header>